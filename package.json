{
  "name": "astradio-mvp",
  "version": "0.2.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20.12.2",
    "npm": ">=10.0.0"
  },
  "description": "Astradio â€“ single-process server (static + ephemeris via Swiss Ephemeris)",
  "main": "server/index.js",
  "scripts": {
    "start": "node staging-server.js",
    "dev": "nodemon server/index.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "vnext:build": "tsc -p vnext/tsconfig.json",
    "build:server": "tsc -p tsconfig.server.json",
    "typecheck": "tsc --noEmit",
    "typecheck:server": "tsc -p tsconfig.server.json --noEmit",
    "vnext:label": "node dist/vnext/teacher/label-from-rules.js",
    "vnext:smoke": "node dist/vnext/scripts/smoke.js",
    "vnext:materialize": "node dist/vnext/scripts/materialize-snapshots.js",
    "vnext:audition": "node dist/vnext/scripts/run-audition.js",
    "vnext:train": "node dist/vnext/scripts/train-student.js",
    "vnext:train-v2": "node dist/vnext/scripts/train-student-v2.js",
    "vnext:train-v1": "node dist/vnext/scripts/train-student-v1.js",
    "vnext:mock-model": "node dist/vnext/scripts/create-mock-model.js",
    "verify:nomocks": "node dist/scripts/check-placeholders.js",
    "verify:fallback": "node dist/scripts/check-fallback-share.js",
    "verify:realml": "npm run vnext:build && npm run vnext:audition && npm run verify:nomocks && npm run verify:fallback",
    "test:astro-coupling": "npm run vnext:build && node dist/vnext/scripts/test-astro-coupling.js",
    "test:wheel-contract": "npm run vnext:build && node dist/vnext/scripts/test-wheel-contract.js",
    "freeze-eval-set": "npm run vnext:build && node dist/vnext/scripts/freeze-eval-set.js",
    "model-provenance": "npm run vnext:build && node dist/vnext/scripts/model-provenance.js",
    "phase2c-eval": "npm run vnext:build && node dist/vnext/scripts/run-phase2c-eval.js",
    "generate-snapshots": "npm run vnext:build && node dist/vnext/scripts/generate-real-snapshots.js",
    "ci-path-guard": "npm run vnext:build && node dist/vnext/scripts/ci-path-guard.js",
    "verify-teacher": "npm run vnext:build && node dist/vnext/scripts/verify-teacher-consistency.js",
    "enforce-thresholds": "npm run vnext:build && node dist/vnext/scripts/enforce-quality-thresholds.js",
    "canary-promotion": "npm run vnext:build && node dist/vnext/scripts/canary-promotion.js",
    "test:pipeline": "npm run test:astro-coupling && npm run test:wheel-contract",
    "ci:vnext": "npm run vnext:build && npm run vnext:materialize && npm run vnext:audition",
    "ci:quality": "npm run ci:vnext && npm run test:pipeline && npm run ci-path-guard && npm run enforce-thresholds",
    "ci:full": "npm run ci:quality && npm run verify-teacher && npm run freeze-eval-set eval",
    "ui:dev": "next dev -p 3001",
    "ui:build": "next build",
    "ui:start": "next start -p 3001",
    "typecheck:server": "tsc -p tsconfig.server.json --noEmit",
    "test:contract": "jest tests/engine-adapter.contract.test.ts",
    "test:e2e": "playwright test tests/e2e.smoke.spec.ts",
    "test:integration": "node scripts/test-integration.js",
    "test:all": "npm run test:contract && npm run test:e2e",
    "smoke:server": "node scripts/smoke-test.js",
    "build:dev": "NODE_ENV=development npm run vnext:build",
    "build:prod": "NODE_ENV=production npm run vnext:build",
    "build:ci": "NODE_ENV=ci npm run vnext:build",
    "verify:deterministic": "node scripts/build-hash.js",
    "check:single-compose": "node scripts/check-single-compose.js",
    "worker:export": "node workers/export-worker.js"
  },
  "dependencies": {
    "@simplewebauthn/server": "^8.3.6",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "argon2": "^0.31.2",
    "aws-sdk": "^2.1490.0",
    "clamscan": "^2.0.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-http-proxy": "^1.6.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.3",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.0-rc.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "redis": "^4.6.10",
    "speakeasy": "^2.0.0",
    "swisseph": "^0.5.17",
    "tone": "^15.1.22",
    "tzlookup": "^1.4.0",
    "ulid": "^2.3.0",
    "uuid": "^11.1.0",
    "wav-encoder": "^1.3.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.22.4",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.0",
    "zustand": "^4.4.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.5",
    "nodemon": "^3.1.0",
    "typescript": "^5.9.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "@playwright/test": "^1.40.0"
  }
}